{
  "name": "alexandria",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.26.0",
  "scripts": {
    "dev": "vite",
    "dev:start": "pnpm tauri dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "test": "pnpm run front:test && pnpm run back:test",
    "front:test": "vitest",
    "back:test": "cargo test --manifest-path src-tauri/Cargo.toml"
  },
  "lint-staged": {
    "**/!(*.svelte).ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.{js,svelte,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src-tauri/**/*.rs": [
      "bash -lc 'cd src-tauri && cargo fmt'"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.2.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@tauri-apps/cli": "^2.10.0",
    "@tsconfig/svelte": "^5.0.6",
    "@types/node": "^24.10.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.14.0",
    "globals": "^17.1.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "sass-embedded": "^1.97.3",
    "svelte": "^5.43.8",
    "svelte-check": "^4.3.4",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "npm:rolldown-vite@7.2.5",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  },
  "dependencies": {
    "@tailwindplus/elements": "^1.0.22",
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-log": "~2"
  }
}
